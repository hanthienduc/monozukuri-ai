# React Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8080

# Firebase Configuration
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_project_id.firebaseapp.com

# Application Configuration
VITE_APP_NAME=Manufacturing Inquiry Assistant
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered manufacturing inquiry assistant

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=false

# Development
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Security
VITE_CSP_NONCE=development-nonce